textures/fhdoom/lena_dxt1
{
    diffusemap textures/glsltest/dds/lena_dxt1.dds
}

textures/fhdoom/lena_dxt1_alpha
{
    twosided
   
    {
        blend diffusemap
        map textures/glsltest/dds/lena_dxt1_alpha.dds
        alphaTest 0.5
    } 
}

